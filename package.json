{
  "name": "genpw",
  "version": "2.0.4",
  "description": "Generate a password.",
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw/issues"
  },
  "license": "MIT",
  "author": "Matthew <matthew@mi11er.net> (https://mi11er.net)",
  "files": [
    "bin",
    "lib",
    "index.js",
    "index.mjs",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "main": "./index.js",
  "module": "./lib/passwordgenerator.mjs",
  "bin": {
    "genpw": "./bin/exec.mjs"
  },
  "repository": "https://github.com/genpw/genpw.git",
  "scripts": {
    "clean": "rimraf .nyc_output coverage dist/* npm-debug.log",
    "codeclimate-test-reporter":
      "codeclimate-test-reporter < coverage/lcov.info",
    "precommit": "run-s lint-staged pretty-quick",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "pretest": "run-s lint",
    "test": "nyc mocha --require esm test/*.mjs",
    "posttest": "nyc report --reporter=lcov",
    "pretty-quick": "pretty-quick --staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "esm": "3.0.16",
    "sjcl": "1.0.7"
  },
  "devDependencies": {
    "@mi11er/eslint-config": "2.2.2",
    "@semantic-release/changelog": "2.0.1",
    "@semantic-release/git": "4.0.1",
    "chai": "4.1.2",
    "codeclimate-test-reporter": "0.5.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-optimize-regex": "1.1.5",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.4",
    "mocha": "5.0.5",
    "npm-run-all": "4.1.2",
    "nyc": "11.6.0",
    "prettier": "1.11.1",
    "pretty-quick": "1.4.1",
    "rimraf": "2.6.2",
    "semantic-release": "15.1.5",
    "sort-package-json": "1.11.0"
  },
  "nyc": {
    "include": ["lib/", "bin/", "index.js", "index.mjs"],
    "require": ["esm"],
    "extension": [".jsx", ".mjs"]
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "tarballDir": "release"
      },
      {
        "path": "@semantic-release/git",
        "assets": ["**"]
      }
    ],
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "release/*.tgz",
            "label": "Package tarball"
          }
        ]
      }
    ]
  }
}
