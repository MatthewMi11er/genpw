{
  "description": "Generate a password.",
  "name": "genpw",
  "version": "1.0.6",
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw/issues"
  },
  "license": "MIT",
  "author": "Matthew <matthew@mi11er.net> (https://mi11er.net)",
  "files": [
    "lib/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "./lib/passwordgenerator.js",
  "bin": {
    "genpw": "./lib/exec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.git"
  },
  "scripts": {
    "prepublish": "run-s clean build:lib",
    "pretest": "npm run --silent lint",
    "test": "nyc mocha",
    "posttest": "nyc report --reporter=lcov",
    "lint": "eslint .",
    "build": "run-s clean build:*",
    "build:lib": "babel -d lib src/js/lib",
    "clean": "rimraf .nyc_output coverage lib npm-debug.log"
  },
  "dependencies": {
    "sjcl": "1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.3.3",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.1.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4"
  },
  "nyc": {
    "include": [
      "src/"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "all": true
  }
}
