{
  "description": "Generate a password.",
  "name": "genpw",
  "release": {
    "branch": "master"
  },
  "homepage": "https://genpw.com",
  "bugs": {
    "url": "https://github.com/genpw/genpw/issues"
  },
  "license": "MIT",
  "author": "Matthew <matthew@mi11er.net> (https://mi11er.net)",
  "files": [
    "lib/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "./lib/passwordgenerator.js",
  "bin": {
    "genpw": "./lib/exec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/genpw/genpw.git"
  },
  "scripts": {
    "prepublish": "run-s clean build:lib",
    "pretest": "npm run --silent lint",
    "test": "nyc mocha",
    "posttest": "nyc report --reporter=lcov",
    "lint": "eslint .",
    "build": "run-s clean build:*",
    "build:lib": "babel -d lib src/js/lib",
    "clean": "rimraf .nyc_output coverage lib npm-debug.log",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "sjcl": "1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^3.1.0",
    "nyc": "^8.3.2",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5"
  },
  "nyc": {
    "include": [
      "src/"
    ],
    "require": [
      "babel-register"
    ],
    "all": true
  }
}
